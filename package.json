    "tippy.js": "^2.5.2",
    "winston": "^3.0.0-rc3",
    "xpath": "0.0.27",
    "xtraverse": "^0.1.0"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2",
    "babel-brunch": "~6.0",
    "babel-preset-latest": "^6",
    "brunch": "^2",
    "clean-css-brunch": "^2",
    "mocha": "^5.1.0",
