{
  "name": "brunch-app",
  "description": "Brunch.io application",
  "private": true,
  "author": "Brunch",
  "version": "0.0.1",
  "repository": "",
  "scripts": {
    "build": "./node_modules/.bin/brunch build --production",
    "prestart": "npm run build",
